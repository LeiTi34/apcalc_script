define C(n,k) = binomial(n,k)
define Cvert(n,k,q) = C(n,k)*(q^k)*(1-q)^(n-k)
define Cvertsum(n, n0,k,q)
{
	local sum = 0;
	while(n>=n0)
	{
		sum += Cvert(n,k,q);
		n--;
	}
	return sum;
}

define H(M, N, n, k) = C(M,k)*C(N-M,n-k)/C(N,n)
define Hsum(M,N,n,k,k0) 
{
	local sum = 0;
	while(k>=k0)
	{
		sum += H(M, N, n, k);
		k--;
	}
	return sum;
}
